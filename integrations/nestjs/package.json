{
  "name": "@willsoto/nestjs-node-konfig",
  "version": "1.0.3",
  "description": "Node Konfig integration package for usage with Nest.js",
  "keywords": [
    "config",
    "configuration",
    "yaml",
    "toml",
    "json",
    "configuration-management",
    "nestjs",
    "typescript",
    "nest"
  ],
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist coverage .nyc_output docs temp *.tsbuildinfo",
    "lint": "eslint . --ext ts,js --fix",
    "predocs": "npm run build",
    "test": "mocha",
    "pretest:cov": "rimraf coverage .nyc_output",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch"
  },
  "nyc": {
    "extends": "@node-konfig-tools/nyc",
    "include": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "@willsoto/node-konfig-core": "workspace:*"
  },
  "devDependencies": {
    "@nestjs/common": "~7.0.0",
    "@nestjs/core": "~7.6.15",
    "@nestjs/testing": "~7.6.15",
    "@node-konfig-tools/eslint": "workspace:*",
    "@node-konfig-tools/mocha": "workspace:*",
    "@node-konfig-tools/nyc": "workspace:*",
    "@rushstack/eslint-config": "~2.3.2",
    "@types/chai": "~4.2.16",
    "@types/chai-as-promised": "~7.1.3",
    "@types/mocha": "~8.2.2",
    "@types/node": "~14.14.37",
    "@types/sinon": "~9.0.11",
    "@types/sinon-chai": "~3.2.5",
    "eslint": "~7.23.0",
    "mocha": "~8.3.2",
    "nyc": "~15.1.0",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.2",
    "rxjs": "~6.6.7",
    "typescript": "~4.2.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.15"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
