{
  "name": "@willsoto/node-konfig-flag",
  "version": "1.0.1",
  "description": "Node Konfig integration for Flags (CLI)",
  "keywords": [
    "config",
    "configuration",
    "yaml",
    "toml",
    "json",
    "configuration-management",
    "http",
    "fetch"
  ],
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist coverage .nyc_output docs temp *.tsbuildinfo",
    "lint": "eslint . --ext ts,js --fix",
    "predocs": "npm run build",
    "test": "mocha",
    "pretest:cov": "rimraf coverage .nyc_output",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch"
  },
  "nyc": {
    "extends": "@node-konfig-tools/nyc",
    "include": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "meow": "~9.0.0"
  },
  "devDependencies": {
    "@node-konfig-tools/eslint": "workspace:*",
    "@node-konfig-tools/mocha": "workspace:*",
    "@node-konfig-tools/nyc": "workspace:*",
    "@rushstack/eslint-patch": "~1.1.0",
    "@types/chai": "~4.2.22",
    "@types/chai-as-promised": "~7.1.4",
    "@types/mocha": "~9.0.0",
    "@types/node": "~16.11.11",
    "@types/sinon": "~10.0.6",
    "@types/sinon-chai": "~3.2.6",
    "@willsoto/node-konfig-core": "workspace:*",
    "eslint": "~8.3.0",
    "mocha": "~9.1.3",
    "nyc": "~15.1.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.5.2"
  },
  "peerDependencies": {
    "@willsoto/node-konfig-core": "^4.0.1"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
