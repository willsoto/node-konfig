{
  "name": "@willsoto/node-konfig-vault",
  "version": "1.0.8",
  "description": "Node Konfig integration for Vault",
  "keywords": [
    "config",
    "configuration",
    "yaml",
    "toml",
    "json",
    "configuration-management",
    "vault"
  ],
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist temp *.tsbuildinfo",
    "lint": "eslint . --ext ts,js --fix",
    "test": "mocha",
    "pretest:cov": "rimraf coverage .nyc_output",
    "test:cov": "c8 mocha",
    "test:watch": "mocha --watch"
  },
  "c8": {
    "all": true,
    "branches": 90,
    "check-coverage": true,
    "functions": 90,
    "include": [
      "src"
    ],
    "lines": 90,
    "reporter": [
      "text",
      "html"
    ],
    "statements": 90
  },
  "dependencies": {
    "@types/request": "~2.48.7",
    "node-vault": "~0.9.22"
  },
  "devDependencies": {
    "@node-konfig-tools/eslint": "workspace:*",
    "@node-konfig-tools/mocha": "workspace:*",
    "@rushstack/eslint-patch": "~1.1.0",
    "@types/chai": "~4.2.22",
    "@types/chai-as-promised": "~7.1.4",
    "@types/mocha": "~9.0.0",
    "@types/node": "~16.11.11",
    "@types/sinon": "~10.0.6",
    "@types/sinon-chai": "~3.2.6",
    "@willsoto/node-konfig-core": "workspace:*",
    "@willsoto/node-konfig-file": "workspace:*",
    "c8": "~7.10.0",
    "eslint": "~8.3.0",
    "mocha": "~9.1.3",
    "rimraf": "~3.0.2",
    "typescript": "~4.5.2"
  },
  "peerDependencies": {
    "@willsoto/node-konfig-core": "^4.0.1"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
