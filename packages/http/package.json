{
  "name": "@willsoto/node-konfig-http",
  "version": "0.1.6",
  "keywords": [
    "config",
    "configuration",
    "yaml",
    "toml",
    "json",
    "configuration-management",
    "http",
    "fetch"
  ],
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist coverage .nyc_output docs temp",
    "lint": "eslint . --ext ts,js --fix",
    "predocs": "npm run build",
    "test": "mocha",
    "pretest:cov": "rimraf coverage .nyc_output",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch"
  },
  "nyc": {
    "extends": "@node-konfig-tools/nyc",
    "include": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "node-fetch": "~2.6.1"
  },
  "devDependencies": {
    "@node-konfig-tools/eslint": "workspace:*",
    "@node-konfig-tools/mocha": "workspace:*",
    "@node-konfig-tools/nyc": "workspace:*",
    "@rushstack/eslint-config": "~2.3.2",
    "@types/chai": "~4.2.15",
    "@types/chai-as-promised": "~7.1.3",
    "@types/lodash": "~4.14.168",
    "@types/mocha": "~8.2.1",
    "@types/node": "~14.14.32",
    "@types/node-fetch": "~2.5.8",
    "@types/sinon": "~9.0.11",
    "@types/sinon-chai": "~3.2.5",
    "@willsoto/node-konfig-core": "workspace:*",
    "eslint": "~7.21.0",
    "lodash": "~4.17.21",
    "mocha": "~8.3.1",
    "nock": "~13.0.11",
    "nyc": "~15.1.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.2.3"
  },
  "peerDependencies": {
    "@willsoto/node-konfig-core": "^0.3.1"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
