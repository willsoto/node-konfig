{
  "name": "@willsoto/node-konfig-core",
  "version": "4.1.0",
  "description": "Core configuration pacakge supporting file, static and environment variables",
  "keywords": [
    "config",
    "configuration",
    "yaml",
    "toml",
    "json",
    "configuration-management"
  ],
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "test": "ava",
    "test:coverage": "c8 ava",
    "test:watch": "ava --watch"
  },
  "c8": {
    "all": true,
    "branches": 90,
    "check-coverage": true,
    "exclude": [
      "src/types.ts"
    ],
    "functions": 90,
    "include": [
      "src"
    ],
    "lines": 90,
    "reporter": [
      "text",
      "html"
    ],
    "statements": 90
  },
  "dependencies": {
    "cockatiel": "~2.0.2",
    "lodash": "~4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.47",
    "@types/sinon": "10.0.13",
    "ava": "4.3.1",
    "c8": "7.12.0",
    "rimraf": "3.0.2",
    "sinon": "14.0.0",
    "typescript": "4.5.5"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
