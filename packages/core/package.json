{
  "name": "@willsoto/node-konfig-core",
  "version": "0.2.0",
  "keywords": [
    "config",
    "configuration",
    "yaml",
    "toml",
    "json",
    "configuration-management"
  ],
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist coverage .nyc_output docs temp",
    "lint": "eslint . --ext ts,js --fix",
    "predocs": "npm run build",
    "test": "mocha",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch"
  },
  "nyc": {
    "extends": "@node-konfig-tools/nyc",
    "include": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "cockatiel": "~2.0.1",
    "js-yaml": "~4.0.0",
    "lodash.setwith": "~4.3.2",
    "meow": "~9.0.0",
    "toml": "~3.0.0"
  },
  "devDependencies": {
    "@node-konfig-tools/eslint": "workspace:*",
    "@node-konfig-tools/mocha": "workspace:*",
    "@node-konfig-tools/nyc": "workspace:*",
    "@rushstack/eslint-config": "~2.3.2",
    "@types/chai": "~4.2.14",
    "@types/chai-as-promised": "~7.1.3",
    "@types/js-yaml": "~4.0.0",
    "@types/lodash": "~4.14.167",
    "@types/lodash.setwith": "~4.3.6",
    "@types/mocha": "~8.2.0",
    "@types/node": "~14.14.20",
    "@types/sinon": "~9.0.10",
    "@types/sinon-chai": "~3.2.5",
    "eslint": "~7.17.0",
    "lodash": "~4.17.20",
    "mocha": "~8.2.1",
    "nyc": "~15.1.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.1.3"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
