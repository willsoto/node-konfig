  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist coverage .nyc_output docs temp",
    "lint": "eslint . --ext ts,js --fix",
    "predocs": "npm run build",
    "test": "mocha",
    "pretest:cov": "rimraf coverage .nyc_output",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch"
  },
  "nyc": {
    "extends": "@node-konfig-tools/nyc",
    "include": [
      "src/**/*.ts"
    ]
  },
  "devDependencies": {
    "@node-konfig-tools/eslint": "workspace:*",
    "@node-konfig-tools/mocha": "workspace:*",
    "@node-konfig-tools/nyc": "workspace:*",
    "@rushstack/eslint-config": "~2.3.1",
    "@types/chai": "~4.2.14",
    "@types/chai-as-promised": "~7.1.3",
    "@types/consul": "~0.23.34",
    "@types/lodash": "~4.14.165",
    "@types/mocha": "~8.0.4",
    "@types/node": "~14.14.9",
    "@types/sinon": "~9.0.8",
    "@types/sinon-chai": "~3.2.5",
    "@willsoto/node-konfig-core": "workspace:*",
    "consul": "~0.39.0",
    "eslint": "~7.14.0",
    "lodash": "~4.17.20",
    "mocha": "~8.2.1",
    "nyc": "~15.1.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.1.2"
  },
  "peerDependencies": {
    "@willsoto/node-konfig-core": "^0.1.1",
    "consul": "~0.39.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
