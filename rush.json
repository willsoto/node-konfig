{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.56.0",
  "pnpmVersion": "6.23.1",
  "pnpmOptions": {
    "strictPeerDependencies": true,
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=14",
  "ensureConsistentVersions": true,

  "gitPolicy": {
    "versionBumpCommitMessage": "chore: applying package updates. [skip-ci]"
  },

  "repository": {
    "url": "git@github.com:willsoto/node-konfig.git",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },

  "telemetryEnabled": false,
  "projects": [
    {
      "packageName": "@willsoto/node-konfig-core",
      "projectFolder": "packages/core",
      "shouldPublish": true
    },
    {
      "packageName": "@willsoto/node-konfig-vault",
      "projectFolder": "loaders/vault",
      "shouldPublish": true
    },
    {
      "packageName": "@willsoto/node-konfig-file",
      "projectFolder": "loaders/file",
      "shouldPublish": true
    },
    {
      "packageName": "@willsoto/node-konfig-flag",
      "projectFolder": "loaders/flag",
      "shouldPublish": true
    },
    {
      "packageName": "@willsoto/node-konfig-consul",
      "projectFolder": "loaders/consul",
      "shouldPublish": true
    },
    {
      "packageName": "@willsoto/node-konfig-http",
      "projectFolder": "loaders/http",
      "shouldPublish": true
    },
    {
      "packageName": "@willsoto/node-konfig-etcd",
      "projectFolder": "loaders/etcd",
      "shouldPublish": true
    },
    {
      "packageName": "@willsoto/node-konfig-yaml-parser",
      "projectFolder": "parsers/yaml",
      "shouldPublish": true
    },
    {
      "packageName": "@willsoto/node-konfig-toml-parser",
      "projectFolder": "parsers/toml",
      "shouldPublish": true
    },
    {
      "packageName": "@node-konfig-tools/mocha",
      "projectFolder": "tools/mocha",
      "reviewCategory": "tools",
      "shouldPublish": false
    },
    {
      "packageName": "@node-konfig-tools/nyc",
      "projectFolder": "tools/nyc",
      "reviewCategory": "tools",
      "shouldPublish": false
    },
    {
      "packageName": "@node-konfig-tools/eslint",
      "projectFolder": "tools/eslint",
      "reviewCategory": "tools",
      "shouldPublish": false
    }
  ]
}
